  "grammars": [
    {
      "name": "alex",
      "camelcase": "Alex",
      "title": "Alex",
      "scope": "source.alex",
      "file-types": [
        "x"
      ],
      "injection-regex": "^alex$",
      "class-name": "TreeSitterAlex"
    }
