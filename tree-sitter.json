  "metadata": {
    "version": "0.1.0",
    "license": "BSD 3-Clause",
    "description": "Alex grammar for tree-sitter",
    "authors": [
      {
        "name": "Brandon Chinn",
        "email": "brandonchinn178@gmail.com",
        "url": "https://brandonchinn178.github.io"
      }
    ],
    "links": {
      "repository": "https://github.com/brandonchinn178/tree-sitter-alex"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "java": false,
    "node": true,
    "python": false,
    "rust": true,
    "swift": false,
    "zig": false
